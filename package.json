{
  "name": "gulag-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "deploy": "pm2 deploy process.yml production",
    "deploy:setup": "pm2 deploy process.yml production setup",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "db:migrate": "knex migrate:latest --knexfile ./db/config.js",
    "db:migration": "knex migrate:make --knexfile ./db/config.js",
    "db:rollback": "knex migrate:rollback --knexfile ./db/config.js",
    "db:seed": "knex seed:run --knexfile ./db/config.js"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "debounce": "1.0.0",
    "moment": "2.17.1",
    "normalize.css": "5.0.0",
    "ramda": "0.23.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-markdown": "2.4.4",
    "react-router-dom": "4.0.0-beta.5",
    "react-select": "1.0.0-rc.2",
    "react-text-mask": "0.19.0",
    "styled-components": "1.4.3"
  },
  "devDependencies": {
    "knex": "0.12.6",
    "pg": "6.1.2",
    "pm2": "2.3.0",
    "react-scripts": "0.8.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "homepage": "/admin",
  "proxy": "http://gulag.urbica.co"
}
