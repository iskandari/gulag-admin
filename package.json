{
  "name": "gulag",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint src",
    "commit": "git-cz",
    "release": "standard-version",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "db:migrate": "knex migrate:latest --knexfile ./db/config.js",
    "db:migration": "knex migrate:make --knexfile ./db/config.js",
    "db:rollback": "knex migrate:rollback --knexfile ./db/config.js",
    "db:seed": "knex seed:run --knexfile ./db/config.js"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "classnames": "2.2.5",
    "d3-array": "1.2.0",
    "d3-axis": "1.0.7",
    "d3-drag": "1.1.0",
    "d3-scale": "1.0.6",
    "d3-selection": "1.0.5",
    "debounce": "1.0.2",
    "history": "4.6.1",
    "isomorphic-fetch": "2.2.1",
    "moment": "2.18.1",
    "normalize.css": "7.0.0",
    "prop-types": "15.5.10",
    "ramda": "0.24.1",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-markdown": "2.5.0",
    "react-router-dom": "4.1.1",
    "react-select": "1.0.0-rc.4",
    "react-text-mask": "5.0.2",
    "react-transition-group": "1.1.3",
    "styled-components": "2.1.2"
  },
  "devDependencies": {
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.0.0",
    "eslint": "4.4.1",
    "eslint-config-airbnb": "15.1.0",
    "eslint-plugin-compat": "1.0.4",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.2.1",
    "react-scripts": "1.0.11"
  },
  "proxy": "http://gulag.urbica.co",
  "browserslist": [
    "> 1%"
  ]
}
